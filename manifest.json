{
  "name": "TagProReplays",
  "description": "Save Replays from within TagPro Games",
  "version": "1.2.26",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "migrations.js",
      "indexedDBUtils.js",
      "data.js",
      "messaging.js",
      "textures.js",
      "background.js",
      "map_draw_functions.js",
      "filesystem.js",
      "lib/whammy.js",
      "lib/jquery.js",
      "lib/jszip.min.js",
      "lib/FileSaver.min.js",
      "lib/sanitize.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.koalabeast.com/*",
        "http://*.newcompte.fr/*",
        "http://tangent.jukejuice.com/*"
      ],
      "exclude_matches": [
        "http://support.koalabeast.com/*"
      ],
      "js": [
        "lib/jquery.js",
        "lib/bootstrap.min.js",
        "lib/jquery.actual.min.js",
        "lib/jquery-ui.min.js",
        "barrier.js",
        "cookies.js",
        "messaging.js",
        "textures.js",
        "map_draw_functions.js",
        "ui/viewer.js",
        "ui/menu.js",
        "TagProReplays.js"
      ],
      "css": [
        "lib/bootstrap.css",
        "lib/jquery-ui.min.css",
        "lib/spinkit.css",
        "ui/menu.css",
        "ui/viewer.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "replayRecording.js",
    "img/*.png",
    "ui/*.css",
    "ui/*.html",
    "lib/images/*.png",
    "lib/glyphicons-halflings-regular.woff"
  ]
}
